  <div class="contlist">  
<div class="row">
  <div class="col-4">
    <div class="list-group" id="list-tab" role="tablist">
      <a class="list-group-item list-group-item-action active" id="list-users-list" data-toggle="list" href="#list-users" role="tab" aria-controls="users">Users</a>
      <a class="list-group-item list-group-item-action" id="list-bars-list" data-toggle="list" href="#list-bars" role="tab" aria-controls="bars">Bars</a>
      <a class="list-group-item list-group-item-action" id="list-drinks-list" data-toggle="list" href="#list-drinks" role="tab" aria-controls="drinks">Drinks</a>
      
    </div>
  </div>
  <div class="col-8">
    <div class="tab-content" id="nav-tabContent">
      <div class="tab-pane fade show active" id="list-users" role="tabpanel" aria-labelledby="list-users-list">
  
        {{#each users}}
        <div>
        <h3>{{username}}</h3>
        <span>
          <a href="/delete/{{_id}}" class="btn btn btn-secondary">Delete</a>
        </span>
        {{/each}}
  
      </div>
      <div class="tab-pane fade" id="list-bars" role="tabpanel" aria-labelledby="list-bars-list">
        {{#each places}}
        <h3>{{name}}</h3>
        <span>
          <a href="/delete/{{_id}}" class="btn btn btn-secondary">Delete</a>
        </span>
        {{/each}}
        </div>

      <div class="tab-pane fade" id="list-drinks" role="tabpanel" aria-labelledby="list-drinks-list">
        {{#each drinks}}
        <h3>{{mark}}</h3>
        <span>
          <a href="/delete/{{_id}}" class="btn btn btn-secondary">Delete</a>
        </span>
        {{/each}}
      </div>
  
    </div>
  </div>
